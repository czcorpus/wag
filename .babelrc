{
	"presets": [
		"@babel/typescript",
		"@babel/react",
		["@babel/env", { "modules": false }]
	],
	"plugins": [
		"@babel/proposal-class-properties",
		"@babel/proposal-object-rest-spread",
		[
			"babel-plugin-styled-components",
			{
			  	"ssr": false
			}
		],
		"@babel/plugin-syntax-dynamic-import",
		"babel-plugin-dynamic-import-webpack"
	]
}
