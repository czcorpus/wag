{
	"presets": [
		["@babel/env", { "modules": false }],
		"@babel/react",
		"@babel/typescript"
	],
	"plugins": [
		[
            "babel-plugin-styled-components",
            {
                "ssr": true,
                "displayNames": false
            }
        ],
		"@babel/proposal-class-properties",
		"@babel/proposal-object-rest-spread",
		"@babel/plugin-syntax-dynamic-import",
		"babel-plugin-dynamic-import-webpack"
	]
}