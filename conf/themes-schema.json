            "required": [
                "category",
                "categoryOther",
                "cmpCategory",
                "geoAreaSpotFillColor",
                "lineChartColor1",
                "lineChartColor2",
                "lineConfidenceAreaColor1",
                "lineConfidenceAreaColor2",
                "scale",
                "themeId",
                "themeLabel"
            ],
            "type": "object"
        }
    },
    "properties": {
        "default": {
            "type": "string"
        },
        "themes": {
            "items": {
                "$ref": "#/definitions/ColorTheme"
            },
            "type": "array"
        }
    },
    "required": [
        "default",
        "themes"
    ],
    "type": "object"
